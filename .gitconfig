[core]
	autocrlf = input
	filemode = false
[user]
	name = Dmitrii Fediuk
	email = dfediuk@gmail.com
[difftool "sourcetree"]
	cmd = '' \"$LOCAL\" \"$REMOTE\"
[mergetool "sourcetree"]
	cmd = "'' "
	trustExitCode = true
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[credential]
	helper = store
[credential "helperselector"]
	selected = <no helper>
[safe]
	directory = %(prefix)///wsl.localhost/Ubuntu-22.04/
